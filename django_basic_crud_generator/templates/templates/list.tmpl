<h2 class="form-title">${model_name}</h2>

<table class="table table-fluid">
    <thead>
        <tr>
            <th>${model_name}</th>
            <th>View</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        {% for ${model_name} in object_list %}
        <tr>
            <td>{{ ${model_name} }}</td>
            <td><a href="{% url "${model_name_u_lower}_detail" ${model_name}.id %}">view</a></td>
            <td><a href="{% url "${model_name_u_lower}_update" ${model_name}.id %}">edit</a></td>
            <td><a href="{% url "${model_name_u_lower}_delete" ${model_name}.id %}">delete</a></td>
        </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{% url "${model_name_u_lower}_create" %}">New</a>
